{
  "type": "module",
  "name": "dicom-processor",
  "version": "2.0.0",
  "description": "A Node.js application for processing DICOM files, converting them to JPG, VTI, NRRD, NIfTI, STL, and VTK formats, and providing viewers for the converted files.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js"
  },
  "dependencies": {
    "canvas": "^3.1.0",
    "dcmjs": "^0.38.1",
    "dicom-parser": "^1.8.11",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "keywords": [
    "DICOM",
    "file upload",
    "image processing",
    "vtk"
  ],
  "author": "Willy Ci",
  "license": "ISC"
}
